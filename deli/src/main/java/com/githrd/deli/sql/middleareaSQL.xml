<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
			"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="MiddleDao">

	<select id="selectCode" resultType="middleareaVO" parameterType="string">
	select * from middlearea where city = #{city}
	</select>
    <select id="selectCodeList" resultType="middleareaVO" parameterType="int">
    select * from middlearea where code = #{code}
	</select>
	
	
</mapper>
