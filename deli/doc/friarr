CREATE TABLE friendArr(
    fano NUMBER(4)
        CONSTRAINT FA_FANO_PK PRIMARY KEY,
    famno NUMBER(4)
        CONSTRAINT FA_FAMNO_NN NOT NULL
        CONSTRAINT FA_FAMNO_FK REFERENCES member(mno),
    falat VARCHAR2(100 CHAR)
        CONSTRAINT FA_FALAT_NN NOT NULL
);

INSERT INTO
    friendArr
VALUES(
    (SELECT NVL(MAX(fano) + 1, 1) FROM friendarr), 1001, '(37.5004196071802, 126.911544544308)'
)